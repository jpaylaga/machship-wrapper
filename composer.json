{
    "name": "jpaylaga/machship-wrapper",
    "description": "An API wrapper that showcases my understanding of Machship's API",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Jpaylaga\\MachshipWrapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joshua Paylaga",
            "email": "joshuapaylaga@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.9",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jpaylaga\\MachshipWrapper\\MachshipWrapperServiceProvider"
            ],
            "aliases": {
                "Machship": "Jpaylaga\\MachshipWrapper\\Facades\\Machship"
            }
        }
    }
}